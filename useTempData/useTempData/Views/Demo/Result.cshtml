@{
    Layout = null;
}
@using useTempData.Models;
@using useTempData.Helpers;


@{ 
    var product = TempDataHelper.Get<Product>(TempData, "product") as Product;
}
<html>
<head>
    <title></title>
</head>
<body>
    <h3>Single values</h3>
    <table border="1">
        <tr>
            <td>Age</td>
            <td>@TempData["age"]</td>
        </tr>
        <tr>
            <td>username</td>
            <td>@TempData["username"]</td>
        </tr>
    </table>
    <h3>product info</h3>
    <table border="1">
        <tr>
            <td>Id</td>
            <td>
                @product.Id
            </td>
        </tr>
        <tr>
            <td>Name</td>
            <td>
                @product.Name
            </td>
        </tr>
        <tr>
            <td>Price</td>
            <td>
                @product.Price
            </td>
        </tr>

    </table>
</body>
</html>